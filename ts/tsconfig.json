{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "rootDir": "./",
        "outDir": "../frontend",

        "incremental": true,
        "tsBuildInfoFile": "./project.tsbuildinfo",

        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node"
    }
}
